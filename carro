public class Automovel {/* Classe pai */
    protected String motor;
    protected String pintura;
    protected String cambio;

    public String getPintura() {/*metodo acessor para armazenar a pintura modificada*/
        return pintura;
    }

    public void setPintura(String pintura) {/*metodo acessor para modificar a pintura do automovel*/
        this.pintura = pintura;
    }

    public String getCambio() {/*metodo acessor para armazenar o câmbio modificado (automatico ou manual)*/
        return cambio;
    }

    public void setCambio(String cambio) {/*metodo acessor para modificar o câmbio do automovel (automatico ou manual) */
        this.cambio = cambio;
    }
    public String getMotor() {/*metodo acessor para armazenar o motor do automovel*/
        return motor;
    }

    public void setMotor(String motor) {/*metodo acessor para modificar o motor do automovel*/
        this.motor = motor;
    }
}
